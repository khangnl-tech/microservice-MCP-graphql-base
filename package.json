    "@google-cloud/speech": "^6.0.0",
    "@google-cloud/text-to-speech": "^5.0.0",
    "@google-cloud/translate": "^8.0.0",
    "qdrant": "^1.0.0",
    "exceljs": "^4.4.0",
    "xlsx": "^0.18.5",
    "node-cron": "^3.0.3",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "compression": "^1.7.4",
    "morgan": "^1.10.0"
