{
  "name": "microservice-mcp-graphql",
  "version": "1.0.0",
  "description": "Microservice architecture with GraphQL and MCP protocol support",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:gateway\" \"npm run dev:auth\" \"npm run dev:ai\" \"npm run dev:media\" \"npm run dev:user\" \"npm run dev:notification\" \"npm run dev:orchestrator\"",
    "dev:gateway": "nodemon services/gateway/src/index.js",
    "dev:auth": "nodemon services/auth/src/index.js",
    "dev:ai": "nodemon services/ai/src/index.js",
    "dev:media": "nodemon services/media/src/index.js",
    "dev:user": "nodemon services/user/src/index.js",
    "dev:notification": "nodemon services/notification/src/index.js",
    "dev:orchestrator": "nodemon services/orchestrator/src/index.js",
    "build": "babel src -d dist",
    "start": "node dist/index.js",
    "test": "jest",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix"
  },
  "keywords": [
    "microservice",
    "graphql",
    "mcp",
    "nodejs",
    "express",
    "mongodb",
    "redis"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.0",
    "@babel/node": "^7.22.0",
    "@babel/preset-env": "^7.23.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.51.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1"
  },
  "dependencies": {
    "apollo-server-express": "^3.12.1",
    "express": "^4.18.2",
    "graphql": "^16.8.1",
    "mongoose": "^8.0.0",
    "redis": "^4.6.10",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "socket.io": "^4.7.4",
    "pdf-parse": "^1.1.1",
    "sharp": "^0.32.6",
    "jimp": "^0.22.10",
